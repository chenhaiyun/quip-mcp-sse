# Quip API token (required)
QUIP_TOKEN=your_quip_api_token_here

# Quip API base URL (optional, default is https://platform.quip.com)
QUIP_BASE_URL=https://platform.quip.com

# Server port (optional, default is 8082)
PORT=8082

# Storage path for CSV files (optional, default is ./storage)
QUIP_STORAGE_PATH=./storage
